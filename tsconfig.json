{
  "compilerOptions": {
    "module": "esnext",
    "declaration": true,
    "allowSyntheticDefaultImports": true,
    "jsx": "react-jsx",
    "target": "es2019",
    "outDir": "dist",
    "declarationMap": true,
    "lib": ["ESNext", "dom"],
    "skipLibCheck": true,
    "moduleResolution": "node",
    "strict": true,
    "strictFunctionTypes": true,
    "noImplicitAny": false
  },
  "exclude": ["node_modules", "dist/**/*"]
}
